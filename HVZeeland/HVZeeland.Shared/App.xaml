<Application
    x:Class="HVZeeland.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:HVZeeland">
    
    <Application.Resources>
        <x:String x:Key="ProgressBarIndeterminateForegroundThemeBrush">#096898</x:String>
        <SolidColorBrush x:Key="TextBoxBorderThemeBrush" Color="#096898" />
        <SolidColorBrush x:Key="TextBoxPlaceholderTextThemeBrush" Color="#096898" />
        <SolidColorBrush x:Key="TextBoxForegroundThemeBrush" Color="#096898" />

        <SolidColorBrush x:Key="ButtonBackgroundThemeBrush" Color="Transparent" />
        <SolidColorBrush x:Key="ButtonBorderThemeBrush" Color="Transparent" />
        <SolidColorBrush x:Key="ButtonDisabledBackgroundThemeBrush" Color="Transparent" />
        <SolidColorBrush x:Key="ButtonDisabledBorderThemeBrush" Color="Transparent" />
        <SolidColorBrush x:Key="ButtonDisabledForegroundThemeBrush" Color="White" />
        <SolidColorBrush x:Key="ButtonForegroundThemeBrush" Color="LightGray" />
        <SolidColorBrush x:Key="ButtonPointerOverBackgroundThemeBrush" Color="Transparent" />
        <SolidColorBrush x:Key="ButtonPointerOverForegroundThemeBrush" Color="LightGray" />
        <SolidColorBrush x:Key="ButtonPressedBackgroundThemeBrush" Color="Transparent" />
        <SolidColorBrush x:Key="ButtonPressedForegroundThemeBrush" Color="White" />


    </Application.Resources>
</Application>
